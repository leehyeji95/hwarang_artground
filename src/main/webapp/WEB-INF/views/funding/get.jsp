function date_time() {

  dateWhen = new Date(Number(2015), Number(8), Number(10), Number(23), Number(59), Number(59));
  date = new Date;
  dateNow = new Date(date.getFullYear(), (date.getMonth() + 1), date.getDate(), date.getHours(), date.getMinutes(), date.getSeconds());
  diff = dateWhen.getTime() - dateNow.getTime();
  var diffHour = Math.floor((diff % (60 * 60 * 1000 * 24)) / (1000 * 60 * 60));
  var diffMin = Math.floor(((diff % (60 * 60 * 1000 * 24)) % (60 * 60 * 1000)) / (1000 * 60));
  var diffSec = Math.floor((((diff % (60 * 60 * 1000 * 24)) % (60 * 60 * 1000)) % (60 * 1000)) / 1000);

  if (diffHour < 10) {
      diffHour = "0" + diffHour;
  }
  hour_1 = 90 + (Number(String(diffHour).charAt(0)) * 15);
  hour_2 = 90 + (Number(String(diffHour).charAt(1)) * 15);

  if (diffMin < 10) {
      diffMin = "0" + diffMin;
  }
  min_1 = 90 + (Number(String(diffMin).charAt(0)) * 15);
  min_2 = 90 + (Number(String(diffMin).charAt(1)) * 15);

  if (diffSec < 10) {
      diffSec = "0" + diffSec;
  }
  sec_1 = 90 + (Number(String(diffSec).charAt(0)) * 15);
  sec_2 = 90 + (Number(String(diffSec).charAt(1)) * 15);
  $('.wd-ui-timer-num-small.hour-1').css("background-position-x", "-" + hour_1 + "px");
  $('.wd-ui-timer-num-small.hour-2').css("background-position-x", "-" + hour_2 + "px");
  $('.wd-ui-timer-num-small.minute-1').css("background-position-x", "-" + min_1 + "px");
  $('.wd-ui-timer-num-small.minute-2').css("background-position-x", "-" + min_2 + "px");
  $('.wd-ui-timer-num-small.second-1').css("background-position-x", "-" + sec_1 + "px");
  $('.wd-ui-timer-num-small.second-2').css("background-position-x", "-" + sec_2 + "px");
  setTimeout('date_time();', '1000');
  return true;
}

